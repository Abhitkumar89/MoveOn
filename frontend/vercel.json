{
    "version": 2,
    "name": "moveon-frontend",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "regions": [
        "bom1"
    ],
    "framework": "vite"
}